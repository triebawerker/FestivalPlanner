<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>

    <a href="/musicians/new">new</a>

    <table>
      <% musicians.forEach(function(musician){ %>
        <tr>
          <td><%= musician.name %></td>
          <td><%= musician.band.name %></td>
          <td>
            <a href="musicians/edit/<%= musician._id %>">edit</a>
          </td>
      <% }); %>
    </table>
  </body>
</html>
